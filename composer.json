{
    "name": "laravel-notification-channels/sms77",
    "description": "SMS77 Notifications Channel for Laravel",
    "homepage": "https://github.com/laravel-notification-channels/sms77",
    "license": "MIT",
    "authors": [
        {
            "name": "Maximilian Sch√∂ll",
            "email": "mail@mxschll.com",
            "homepage": "https://mxschll.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.1",
        "illuminate/notifications": "^10.0 || ^11.0",
        "illuminate/support": "^10.0 || ^11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SMS77\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\SMS77\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\SMS77\\SMS77ServiceProvider"
            ]
        }
    }
}
